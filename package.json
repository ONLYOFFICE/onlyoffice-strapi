{
  "name": "onlyoffice-strapi",
  "version": "2.0.0",
  "description": "The ONLYOFFICE plugin allows users to view, edit, and co-author office documents, spreadsheets, and presentations added to the Strapi Media Library using ONLYOFFICE Docs.",
  "type": "commonjs",
  "license": "MIT",
  "strapi": {
    "kind": "plugin",
    "name": "onlyoffice",
    "displayName": "ONLYOFFICE",
    "description": "The ONLYOFFICE plugin allows users to view, edit, and co-author office documents, spreadsheets, and presentations added to the Strapi Media Library using ONLYOFFICE Docs."
  },
  "keywords": [
    "strapi",
    "plugin",
    "onlyoffice",
    "docs",
    "editor",
    "document",
    "spreadsheet"
  ],
  "author": "Ascensio System SIA <integration@onlyoffice.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ONLYOFFICE/onlyoffice-strapi.git"
  },
  "bugs": {
    "url": "https://github.com/ONLYOFFICE/onlyoffice-strapi/issues",
    "email": "support@onlyoffice.com"
  },
  "homepage": "https://github.com/ONLYOFFICE/onlyoffice-strapi#readme",
  "scripts": {
    "prepare": "husky install",
    "prepush": "npm run lint",
    "lint": "npx eslint .",
    "test:unit": "jest --verbose",
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify"
  },
  "files": [
    "admin",
    "server",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "dependencies": {
    "@onlyoffice/document-editor-react": "^2.1.0",
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "byte-size": "^9.0.1",
    "mime-types": "^3.0.1",
    "react-intl": "^7.1.11"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.20.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.3",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "lint-staged": {
    "*.{css,less,scss,html,json,jsx,js,md}": [
      "npx eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "engines": {
    "node": ">=14.19.1 <=22.0.0",
    "npm": ">=6.0.0 <=10.5.1"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.js",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.js",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
